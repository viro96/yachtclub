{
  "name": "Yachtclub-Wendenschloss",
  "version": "1.0.0",
  "main": "server/app.ts",
  "license": "MIT",
  "description": "Yachtclub Website using Angular 4, Angular 2, Express, Mongoose and Node.",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "lint": "ng lint",
    "e2e": "protractor",
    "tsc": "tsc"
  },
  "private": true,
